<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<handlers>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
		</handlers>
		<aspNetCore processPath="dotnet" arguments=".\WebApi-ModuleV2.dll" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" hostingModel="inprocess">
			<handlerSettings>
				<handlerSetting name="debugFile" value=".\logs\aspnetcore-debug.log" />
				<handlerSetting name="debugLevel" value="FILE,TRACE" />
			</handlerSettings>
		</aspNetCore>
	</system.webServer>
</configuration>